{
  "name": "sankey-form",
  "version": "0.0.0",
  "description": "",
  "main": "build/index.js",
  "license": "Apache-2.0",
  "keywords": [],
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "npm run license && eslint --fix --no-error-on-unmatched-pattern src/ test/",
    "license": "license-check-and-add add -f license-config.json",
    "build": "npm run clean && npm run bundle && ncp appsscript.json dist/appsscript.json",
    "bundle": "rollup --no-treeshake -c rollup.config.mjs",
    "clean": "rimraf build dist",
    "deploy": "npm run lint && npm run test && npm run build && ncp .clasp-dev.json .clasp.json && clasp push -f",
    "deploy:prod": "npm run lint && npm run test && npm run build && ncp .clasp-prod.json .clasp.json && clasp push -f"
  },
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@google/clasp": "^3.0.6-alpha",
    "@types/google-apps-script": "^1.0.99",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "gts": "^6.0.2",
    "license-check-and-add": "^4.0.5",
    "ncp": "^2.0.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.43.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-license": "^3.6.0",
    "rollup-plugin-prettier": "^4.1.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@vitest/ui": "^3.2.3",
    "happy-dom": "^18.0.1",
    "vitest": "^3.2.3"
  }
}
